{
    "name": "cityware/city-exception",
    "description": " ",
    "license": "GPL-3.0",
    "keywords": [
        "cityware",
        "exception"
    ],
    "homepage": "https://github.com/cityware/city-exception",
    "autoload": {
        "psr-4": {
            "Cityware\\Exception\\": "src/"
        },
        "psr-0": {
            "Cityware\\Exception\\": "src/"
        }
    },
    "require": {
        "php": ">=5.3.23"
    },
    "autoload": {
      "classmap": [
        "src/"
      ]
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "~4.0"
    },
    "suggest": {
        "ext-intl": "Required for most features of Zend\\I18n; included in default builds of PHP"
    },
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CitywareTest\\Exception\\": "test/"
        }
    }
}